library(readxl)
library(openxlsx)
setwd("~/Documents/Research/Sagarika_Microarray/Rosai_Dorfman")
file = "Roas_case_G_vs_R to Tian corrected sp probes only.xlsx"
dat.table = list(5)
for(i in 1:5) {
dat = read.xlsx(file, sheet=i)
dat.agg1 = aggregate(dat$logFC, by=list(dat$Description), sum)
colnames(dat.agg1) = c("Organism", "sum.logFC")
dat.agg2 = aggregate(dat$adj.Pt.Val, by=list(dat$Description), min)
colnames(dat.agg2) = c("Organism", "min.Pt.Val")
dat.agg = merge(dat.agg1, dat.agg2)
dat.agg = dat.agg[order(dat.agg$sum.logFC, decreasing=T),]
dat.table[[i]] = dat.agg
}
names(dat.table) = excel_sheets(file)
View(dat)
View(dat.agg)
write.xlsx(dat.table, paste(substr(file, 1, nchar(file)-4), "aggregate.xlsx"))
